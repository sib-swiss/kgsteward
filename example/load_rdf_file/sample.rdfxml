    xmlns:xsd="http://www.w3.org/2001/XMLSchema#" > 
  <rdf:Description rdf:nodeID="A0">
    <yearsWorked rdf:datatype="http://www.w3.org/2001/XMLSchema#int">6</yearsWorked>
    <employeeRating rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">27.2</employeeRating>
    <employeeType rdf:resource="http://example.org/PartTimeEmployee"/>
