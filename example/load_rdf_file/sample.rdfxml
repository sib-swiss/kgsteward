  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/WallyWorld">
    <rdfs:label>Wally World Amusements</rdfs:label>
    <rdf:type rdf:resource="http://example.org/Company"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://example.org/Bill">
