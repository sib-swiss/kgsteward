@prefix kgsteward: <https://purl.expasy.org/kgsteward/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix orcid: <https://orcid.org/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

kgsteward: a owl:Ontology ;
    owl:versionIRI kgsteward:ontology_version\/1.0.0 ;
    owl:versionInfo "1.0.0" ;
    dcterms:description "The RDF vocabulary describing concepts used by kgsteward" ;
    dcterms:title "kgsteward RDF vocabulary" ;
    dcterms:license <https://creativecommons.org/licenses/by-nc-sa/4.0/> ;
    dcterms:creator orcid:0000-0001-9292-9463 ;
    dcterms:abstract """
This tiny vocabulary is used by kgsteward to describe the datasets it manages. 
It specializes and extends the void vocabulary with additional properties such as 
checksum and last modification date.
""" .

kgsteward:Dataset a rdfs:Class ;
    rdfs:subClassOf void:Dataset ;
    rdfs:label "kgsteward dataset" .

kgsteward:triples a rdf:Property ;
    rdfs:subPropertyOf void:triples ;
    rdfs:label  "number of triples" ;
    rdfs:domain kgsteward:Dataset ;
    rdfs:range  xsd:integer .
    
kgsteward:modified a rdf:Property ;
    rdfs:subPropertyOf dct:modified ;
    rdfs:label  "last modificationdate" ;
    rdfs:domain kgsteward:Dataset ;
    rdfs:range  xsd:dateTime .

kgsteward:checksum a rdf:Property ;
    rdfs:label  "kgsteward checksum" ;
    rdfs:domain kgsteward:Dataset ;
    rdfs:range  xsd:string .
